<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="all"
	package="org.sakaiproject.scorm.model.api" default-lazy="false">


	<class name="org.sakaiproject.scorm.model.api.ContentPackage"
		table="SCORM_CONTENT_PACKAGE_T">
		<id name="contentPackageId" column="PACKAGE_ID" type="long">
			<generator class="native">
				<param name="sequence">SCORM_UID_S</param>
			</generator>
		</id>

		<property name="title" column="TITLE" />
		<property name="resourceId" column="RESOURCE_ID" />
		<property name="manifestId" column="MANIFEST_ID" />
		<property name="manifestResourceId" column="MANIFEST_RESOURCE_ID" />
		<property name="context" column="CONTEXT" />
		<property name="url" column="URL" />
		<property name="releaseOn" column="RELEASE_ON" />
		<property name="dueOn" column="DUE_ON" />
		<property name="acceptUntil" column="ACCEPT_UNTIL" />
		<property name="createdOn" column="CREATED_ON" />
		<property name="createdBy" column="CREATED_BY" />
		<property name="modifiedOn" column="MODIFIED_ON" />
		<property name="modifiedBy" column="MODIFIED_BY" />
		<property name="numberOfTries" column="NUMBER_OF_TRIES" />
		<property name="startNewAttempt" column="START_NEW_ATTEMPTS" />
		<property name="viewSortOrder" column="VIEW_SORT_ORDER" />
		<property name="deleted" column="IS_DELETED" />
		<property name="getHighestResult" column="HIGHEST_RESULT" />
		<property name="category" column="CATEGORY" />
		<property name="groupsPermission">
			<column name="GROUP_PERMISSION" length="4000" />
		</property>
		<property name="prerequisite">
			<column name="PREREQUISITE" length="4000" />
		</property>
		<property name="nextResourceId" column="NEXT_RESOURCE_ID" />
	</class>





</hibernate-mapping>